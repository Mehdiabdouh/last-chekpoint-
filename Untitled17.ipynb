    "import time\n",
    "timelength=90\n",
    "def coutdown(t):\n",
    "    while t>=0:\n",
    "        mins,secs=divmod(t,59) \n",
    "        timeformat=\"{:01d}:{:02d}\".format(mins,secs)\n",
    "        print(timeformat, end='\\r')\n",
    "        time.sleep(1)\n",
    "        t=t-1\n",
    "    print(\"\\nFire in the hole \")\n",
    "coutdown(timelength)"
